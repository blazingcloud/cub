<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @event.name %>
</p>

<p>
  <b>Description:</b>
  <%= @event.description %>
</p>

<p>
  <b>Start date:</b>
  <%= @event.start_date %>
</p>

<p>
  <b>End date:</b>
  <%= @event.end_date %>
</p>

<p>
  <b>Location:</b>
  <%= @event.location %>
</p>

<p>
  <b>Url:</b>
  <%= @event.url %>
</p>


<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>



<div ng-app="cub">
  <div ng-controller="SessionsController">

    <form ng-submit="addSession()">
      <label for="new_session_name">Create New Session:</label>
      <input id="new_session_name" type="text" ng-model="newSessionName" placeholder="Session Name">
      <input class="btn-primary" type="submit" value="add">
    </form>

    <div class="gridStyle" ng-grid="gridOptions"></div>

  </div>
</div>
